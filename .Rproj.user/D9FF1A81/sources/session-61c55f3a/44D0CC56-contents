  plot_frq(coord.flip =T,LhCSIEmergency1)
Chad_ea_2019 <- Chad_ea_2019 %>% 
  dplyr::mutate(LhCSIEmergency1 = dplyr::case_when(
    LhCSIEmergency1 == 1 ~ "1",
    LhCSIEmergency1 == 2 ~ "2",
    LhCSIEmergency1 == 3 ~ "3",
    LhCSIEmergency1 == 4 ~ "4",
    .default = NA
  ))

Chad_ea_2019 <- Chad_ea_2019 %>% 
  dplyr::mutate(LhCSIEmergency1 = dplyr::recode(LhCSIEmergency1,"1"=1,"2"=2,"3"=3,"4"=4))
