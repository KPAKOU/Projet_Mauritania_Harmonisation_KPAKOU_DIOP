  dplyr::mutate(LhCSIStress4 = dplyr::case_when(
    LhCSIStress4 == 1 ~ "1",
    LhCSIStress4 == 2 ~ "2",
    LhCSIStress4 == 3 ~ "3",
    LhCSIStress4 == 4 ~ "4",
    .default = NA
  ))

Chad_ea_2019 <- Chad_ea_2019 %>% 
  dplyr::mutate(LhCSIStress4 = dplyr::recode(LhCSIStress4,"1"=1,"2"=2,"3"=3,"4"=4))
