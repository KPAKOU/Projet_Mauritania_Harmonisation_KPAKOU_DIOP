    ADMIN1Name  ==  "6"  ~  "Ennedi Ouest",
    ADMIN1Name  ==  "7"  ~  "Guera",
    ADMIN1Name  ==  "8"  ~  "Hadjer-Lamis",
    ADMIN1Name  ==  "9"  ~  "Kanem",
    ADMIN1Name  ==  "10"  ~  "Lac",
    ADMIN1Name  ==  "11"  ~  "Logone Occidental",
    ADMIN1Name  ==  "12"  ~  "Logone Oriental",
    ADMIN1Name  ==  "13"  ~  "Mandoul",
    ADMIN1Name  ==  "14"  ~  "Mayo-Kebbi Est",
    ADMIN1Name  ==  "15"  ~  "Mayo-Kebbi Ouest",
